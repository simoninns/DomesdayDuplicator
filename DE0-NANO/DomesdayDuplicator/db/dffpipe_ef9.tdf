--synthesis_resources = reg 32 
OPTIONS ALTERA_INTERNAL_OPTION = "AUTO_SHIFT_REGISTER_RECOGNITION=OFF";

SUBDESIGN dffpipe_ef9
( 
	clock	:	input;
	clrn	:	input;
	d[15..0]	:	input;
	q[15..0]	:	output;
) 
VARIABLE 
	dffe19a[15..0] : dffe;
	dffe20a[15..0] : dffe;
	ena	: NODE;
	prn	: NODE;
	sclr	: NODE;

BEGIN 
	dffe19a[].clk = clock;
	dffe19a[].clrn = clrn;
	dffe19a[].d = (d[] & (! sclr));
	dffe19a[].ena = ena;
	dffe19a[].prn = prn;
	dffe20a[].clk = clock;
	dffe20a[].clrn = clrn;
	dffe20a[].d = (dffe19a[].q & (! sclr));
	dffe20a[].ena = ena;
	dffe20a[].prn = prn;
	ena = VCC;
	prn = VCC;
	q[] = dffe20a[].q;
	sclr = GND;
END;
